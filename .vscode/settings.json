{
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "java.configuration.updateBuildConfiguration": "automatic",
  "filewatcher.commands": [
    {
      "match": "\\.java",
      "isAsync": true,
      "cmd": "npx prettier --write ${file}",
      "event": "onFileChange"
    }
  ],
  "workbench.tree.indent": 14
}
